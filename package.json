{
  "name": "ai-code",
  "version": "0.0.1",
  "description": "Modify, refactor, and generate code with the help of large language models",
  "repository": "https://github.com/Anemy/ai-code",
  "author": "Anemy",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf dist",
    "webpack": "webpack --config ./webpack.config.js",
    "webpack:watch": "webpack --config ./webpack.config.js --watch",
    "prestart": "npm run clean",
    "start": "npm run webpack && concurrently --kill-others \"npm run webpack:watch\" \"electron ./dist/main.js\"",
    "test": "jest",
    "prettier": "prettier",
    "eslint": "eslint",
    "depcheck": "depcheck",
    "lint": "npm run eslint . && npm run prettier -- --check .",
    "check": "npm run lint && npm run depcheck",
    "reformat": "npm run prettier -- --write . && npm run eslint . --fix"
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@leafygreen-ui/banner": "^7.0.3",
    "@leafygreen-ui/button": "^20.0.1",
    "@leafygreen-ui/card": "^9.0.5",
    "@leafygreen-ui/code": "^14.2.0",
    "@leafygreen-ui/emotion": "^4.0.3",
    "@leafygreen-ui/leafygreen-provider": "^3.1.1",
    "@leafygreen-ui/text-area": "^8.0.5",
    "@leafygreen-ui/text-input": "^12.1.6",
    "@leafygreen-ui/typography": "^16.0.1",
    "@reduxjs/toolkit": "^1.9.3",
    "concurrently": "^6.1.0",
    "dotenv": "^16.0.3",
    "execa": "^7.0.0",
    "glob": "^9.2.1",
    "openai": "^3.2.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "temp": "^0.9.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@leafygreen-ui/tokens": "^2.0.0",
    "@mongodb-js/prettier-config-compass": "^1.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.6",
    "@types/temp": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-jest": "^26.6.3",
    "buffer": "^6.0.3",
    "depcheck": "^1.4.3",
    "electron": "^23.1.2",
    "eslint": "^8.35.0",
    "html-webpack-plugin": "^5.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "typescript": "^4.9.5",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0"
  }
}

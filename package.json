{
  "name": "ai-code",
  "version": "0.0.1",
  "description": "Modify, refactor, and generate code with the help of large language models",
  "repository": "https://github.com/Anemy/ai-code",
  "author": "Anemy",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf dist",
    "webpack": "webpack --config ./webpack.config.js",
    "webpack:watch": "webpack --config ./webpack.config.js --watch",
    "prestart": "npm run clean",
    "start": "npm run webpack && concurrently --kill-others \"npm run webpack:watch\" \"electron ./dist/main.js\"",
    "test": "jest",
    "prettier": "prettier",
    "eslint": "eslint",
    "depcheck": "depcheck",
    "lint": "npm run eslint . && npm run prettier -- --check .",
    "check": "npm run lint && npm run depcheck",
    "reformat": "npm run prettier -- --write . && npm run eslint . --fix"
  },
  "dependencies": {
    "concurrently": "^6.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@mongodb-js/prettier-config-compass": "^1.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-jest": "^26.6.3",
    "css-loader": "^5.2.4",
    "depcheck": "^1.4.3",
    "electron": "^23.1.2",
    "eslint": "^8.35.0",
    "html-webpack-plugin": "^5.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "typescript": "^4.9.5",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0"
  }
}
